{
  "name": "overwatch",
  "version": "1.2.19",
  "description": "Universal Multi-Tenant Management Tool",
  "license": "MIT",
  "author": "marwain91",
  "repository": {
    "type": "git",
    "url": "https://github.com/marwain91/overwatch.git"
  },
  "homepage": "https://github.com/marwain91/overwatch",
  "bugs": {
    "url": "https://github.com/marwain91/overwatch/issues"
  },
  "keywords": [
    "multi-tenant",
    "docker",
    "management",
    "admin-panel",
    "devops"
  ],
  "main": "dist/index.js",
  "bin": {
    "overwatch": "./bin/overwatch.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:ui": "cd ui && npm run dev",
    "build": "tsc",
    "build:ui": "cd ui && npm ci && npm run build",
    "build:all": "tsc && cd ui && npm ci && npm run build",
    "build:cli": "tsc && pkg dist/cli.js --targets node20-linux-x64,node20-linux-arm64 --output dist/bin/overwatch --compress GZip",
    "start": "node dist/index.js",
    "setup": "tsx scripts/setup.ts",
    "setup:check": "tsx scripts/setup.ts --check",
    "setup:force": "tsx scripts/setup.ts --force"
  },
  "pkg": {
    "assets": ["../package.json"],
    "outputPath": "dist/bin"
  },
  "dependencies": {
    "dockerode": "^4.0.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.16.0",
    "node-cron": "^4.2.1",
    "pg": "^8.11.3",
    "inquirer": "^8.2.6",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.10",
    "@types/dockerode": "^3.3.23",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@yao-pkg/pkg": "^6.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
